    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "start": "expo start --clear",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "lint": "expo lint",
        "deps": "expo install --check",
        "console": "hot-updater console",
        "fp": "hot-updater fingerprint",
        "deploy:ios": "hot-updater deploy --platform ios",
        "deploy:android": "hot-updater deploy --platform android",
        "build:android": "eas build --platform android --profile production --local",
        "prebuild:ios": "expo prebuild --platform ios --clean",
        "prebuild:android": "expo prebuild --platform android --clean",
        "eas-build-post-install": "allow-scripts run"
    },
