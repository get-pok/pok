{
    "name": "pocketbase-mobile",
    "version": "1.3.0",
    "description": "The mobile app for PocketBase",
    "author": {
        "name": "Pok",
        "url": "https://github.com/get-pok/pok"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/get-pok/pok"
    },
    "homepage": "https://github.com/get-pok/pok",
    "keywords": [
        "pocketbase",
        "backend",
        "database",
        "realtime",
        "baas",
